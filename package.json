{
  "name": "taskward",
  "description": "âœ… A minimalist Todo&Tasks web application.",
  "author": "Bruce Song <recall4056@gmail.com> (https://github.com/recallwei/)",
  "version": "0.0.0",
  "scripts": {
    "dev:mobile:web": "pnpm -F mobile web",
    "lint:check": "pnpm nx run-many -t type:check cspell:check eslint:check prettier:check --all --parallel=10",
    "type:check": "pnpm nx run-many -t type:check --all",
    "cspell:sort:dicts": "pnpm bo cspell:sort:dicts",
    "cspell:check": "pnpm nx run-many -t cspell:check --all",
    "eslint:check": "pnpm nx run-many -t eslint:check --all",
    "eslint:fix": "pnpm nx run-many -t eslint:fix --all",
    "prettier:check": "pnpm nx run-many -t prettier:check --all",
    "prettier:fix": "pnpm nx run-many -t prettier:fix --all",
    "cz": "git-cz",
    "prepare": "husky"
  },
  "dependencies": {
    "@nx/next": "^19.1.0"
  },
  "devDependencies": {
    "@bit-ocean/cli": "^0.0.19",
    "@bit-ocean/commitlint-config": "^0.0.19",
    "@bit-ocean/cspell": "^0.0.19",
    "@bit-ocean/eslint-config": "^0.0.19",
    "@bit-ocean/prettier-config": "^0.0.19",
    "@bit-ocean/renovate-config": "^0.0.19",
    "@bit-ocean/tsconfig": "^0.0.19",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@nx/workspace": "19.1.0",
    "@taskward/cspell": "workspace:^",
    "commitizen": "^4.3.0",
    "cspell": "^8.8.3",
    "cz-git": "^1.9.2",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.5",
    "nx": "19.1.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "engines": {
    "node": ">=20"
  },
  "private": true,
  "license": "MIT"
}
